{
    "name": "mindtwo/px-user-laravel",
    "description": "Package for handling PX User authentication for Laravel.",
    "keywords": [
        "login",
        "px-user",
        "vnr",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "homepage": "https://github.com/mindtwo/px-user-laravel",
    "authors": [
        {
            "name": "mindtwo GmbH",
            "email": "info@mindtwo.de",
            "homepage": "https://mindtwo.de"
        }
    ],
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.0",
        "mindtwo/two-tility": "^1.0",
        "mindtwo/laravel-decorator": "^3.0",
        "laravel/scout": "^10.13",
        "spatie/laravel-data": "^4.18"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "laravel/sanctum": "^4.0",
        "nunomaduro/collision": "^8.1.1",
        "orchestra/testbench": "^10.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "mockery/mockery": "^1.5",
        "larastan/larastan": "^3.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0"
    },
    "suggest": {
        "laravel/sanctum": "The package provides a custom AuthToken Model which may be used by your application"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "mindtwo\\PxUserLaravel\\": "src/",
            "mindtwo\\PxUserLaravel\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mindtwo\\PxUserLaravel\\PxUserProvider"
            ],
            "aliases": []
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare",
            "@php vendor/bin/testbench package:discover --ansi"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve"
        ],
        "lint": [
            "@php vendor/bin/pint",
            "@php vendor/bin/phpstan analyse"
        ],
        "phpstan": [
            "./vendor/bin/phpstan analyse"
        ],
        "test": [
            "@php vendor/bin/pest"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://repo.packagist.com/mindtwo/"
        }
    ]
}
